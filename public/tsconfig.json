{
  "compilerOptions": {
      "module": "CommonJS",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "./build",
      "declaration": true,
      "baseUrl": ".",
      "paths": {
          "*": [
              "node_modules/*"
          ]
      },
      "resolveJsonModule":true,
  },
  "include": ["./**/*"],
} 